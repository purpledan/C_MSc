# This is an example of a dcode program
# Config block

!_PUMP_0
NAME = Carrier
ADDRESS = 0
SPEED = 1MLPS
1 = WATER0
2 = RED
3 = ETH0
5 = OUTPUT0
6 = WASTE0
@_PUMP_0

!_PUMP_1
ADDRESS = 1
1 = WATER1
2 = BLUE
3 = ETH1
5 = OUTPUT1
6 = WASTE1
@_PUMP_1

# Steps are written like this
!COLOUR
    WATER0->OUTPUT0;2.5ML
    WATER1->OUTPUT1;2.5ML
    SYNC
    RED->OUTPUT0;5ML
    BLUE->OUTPUT1;5ML
    SYNC
    WATER0->OUTPUT0;1ML
    WATER1->OUTPUT1;1ML
@COLOUR

!RINSE
    WATER0->OUTPUT0;5ML
    WATER1->OUTPUT1;5ML
    WATER0->OUTPUT0;5ML
    WATER1->OUTPUT1;5ML
@RINSE

!ETHRINSE
    ETH0->OUTPUT0;2ML
    ETH1->OUTPUT1;2ML
    WATER0->OUTPUT0;5ML
    WATER1->OUTPUT1;5ML
@ETHRINSE


!RUN
COLOUR
@RUN