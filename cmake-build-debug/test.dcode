# This is an example of a dcode program
# Config block

!_HOTPLATE_0
NAME = Radleys
ADDRESS = 0
SPEED = 1000RPM
TEMP = 50C
@_HOTPLATE_0

!_PUMP_0
NAME = Carrier
ADDRESS = 0
SPEED =         0.75MLPS
1 = WATERA0;     0.9MLPS
2 = WATERB0;     0.9MLPS
3 = PREA;        0.75MLPS
4 = OUTPUT0;     4.0MLPM
6 = WASTE0;      0.85MLPS
@_PUMP_0

!_PUMP_1
ADDRESS = 1
SPEED = 0.75MLPS
1 = WATERA1;      0.9MLPS
2 = WATERB1;      0.9MLPS
3 = PREB;        0.75MLPS
4 = OUTPUT1;     4.0MLPM
6 = WASTE1;      0.85MLPS
@_PUMP_1

# Steps are written like this

!PRIME
    PREA->WASTE0;2.5ML
    PREB->WASTE1;2.5ML
    WATERA0->WASTE0;1ML
    WATERA1->WASTE1;1ML
    SYNC
    WATERA0->OUTPUT0;3ML;8MLPM
    WATERA1->OUTPUT1;3ML;8MLPM
    SYNC
@PRIME

!DEPO
    PREA->OUTPUT0;0.6ML
    WATERA0->WASTE0;1ML
    WATERA0->OUTPUT0;1.8ML
    SYNC
    PREB->OUTPUT1;0.6ML
    WATERA1->WASTE1;1ML
    WATERA1->OUTPUT1;1.8ML
    SYNC
@DEPO

!PURGESLOW
WATERA0->OUTPUT0;5ML
WATERA1->OUTPUT1;5ML
SYNC
@PURGESLOW

!PURGEFAST
WATERA0->OUTPUT0;5ML;0.1MLPS
WATERA1->OUTPUT1;5ML;0.1MLPS
SYNC
@PURGEFAST

!RUN
PRIME;1
DEPO;200
PURGEFAST;1
PURGESLOW;10
@RUN